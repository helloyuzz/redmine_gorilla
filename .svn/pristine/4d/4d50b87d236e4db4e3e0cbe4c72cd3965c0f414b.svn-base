@page
@model com.redmine.gorilla.Pages.Progresses.IndexModel

@{
    ViewData["Title"] = "Index";
}
<style>
    td {
        vertical-align: middle !important;
    }
</style>
@*<h1 class="text-shadow">全国省市字典管理</h1>*@
<div class="transprantBackground" style="padding:8px;">
    <form id="searchForm" method="get">
        <table class="table table-hover">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].Version)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].StartPrograming)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].Status)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].PlanComplete)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].RealComplete)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].Source)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].RequirementURL)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].PrototypeURL)
                    </th>
                    <th style="width:18%;">
                        @Html.DisplayNameFor(model => model.AllVersion[0].EmployeeList)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].PublishBy)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.AllVersion[0].ReleasePath)
                    </th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model.AllVersion) {
                <tr>
                    <td>@Html.Raw(item.Version)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.StartPrograming)
                    </td>
                    <td>@Html.Raw(item.Status)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.PlanComplete)
                    </td>
                    <td>@Html.Raw(item.RealComplete)
                    </td>
                    <td>
                        @Html.Raw(item.Source)
                    </td>
                    <td>
                        @Html.Raw(item.RequirementURL)                        
                    </td>
                    <td>
                        @Html.Raw(item.PrototypeURL)
                    </td>
                    <td>
                        @Html.Raw(item.EmployeeList)
                    </td>
                    <td>@Html.Raw(item.PublishBy)
                    </td>
                    <td>
                        @Html.Raw(item.ReleasePath)
                        @*<a href="@Html.DisplayFor(modelItem => item.ReleasePath)" target="_blank">提取升级包</a>*@
                    </td>
                    <td align="right">
                        <div class="dropdown">
                            <button type="button" class="btn btn-primary dropdown-toggle btn-xs"
                                    data-toggle="dropdown">
                                操作
                            </button>
                            <ul class="dropdown-menu pull-right" role="menu" aria-labelledby="dropdownMenu1">
                                <li role="presentation">
                                    @Html.Raw(item.RequirementURL)
                                </li>
                                <li role="presentation" class="divider"></li>
                                <li role="presentation">
                                    <a href="@com.redmine.gorilla.Globals.VersionURL/@item.Id" target="_blank">打开 @item.Name1 主页</a>
                                </li>
                                @*<li role="presentation">
                                    <a asp-page="./Delete" asp-route-id="@item.Id">提取升级包</a>
                                </li>*@
                            </ul>
                        </div>
                    </td>
                </tr>
                }
            </tbody>
        </table>       
        <div style="margin:0px;" class="row">
            <div class="col-1">
            </div>
            <div class="col-xl-2">

            </div>
        </div>
    </form>
    <script>
        function NaviToPage(pageIndex) {
            $("#htmlPageIndex").val(pageIndex);
            $("#searchForm").submit();
        }
    </script>
</div>